//1. Two Sum


class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> res;
        int siz = nums.size();
        for(int i = 0; i < siz - 1 ; i++) {
            for(int j = i + 1; j < siz; j++) {
                if ((nums[i] + nums[j]) == target) {
                    res.push_back(i);
                    res.push_back(j);
                }
            }
        }
        return res;
    }
};
