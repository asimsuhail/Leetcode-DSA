


class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int index = 0;
        for (int i = 0, siz = nums.size() ; i < siz; i++){
            if (nums[i] != val) {
                nums[index++] = nums[i];
            }
        }
        
        return index;
    }
};
