class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        
        
        vector<vector<int>> x;

    for (int i = 0; i < numRows; ++i)
      x.push_back(vector<int>(i + 1, 1));

    for (int i = 2; i < numRows; ++i)
      for (int j = 1; j < x[i].size() - 1; ++j)
        x[i][j] = x[i - 1][j - 1] + x[i - 1][j];

        return x;
    }
};
